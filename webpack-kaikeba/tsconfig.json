{
    "compilerOptions": {
        "target": "ES5",
        "sourceMap": true,
        "jsx": "react",
        "noImplicitThis": true,
        "strictNullChecks": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "downlevelIteration": true,
        "allowJs": true,
        "pretty": true,
        "resolveJsonModule": true,
    },
    "exclude": [
        "node_modules"
    ]
}